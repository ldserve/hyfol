{%- comment -%}--------------------------Mobile--------------------------{%- endcomment -%}
<section class="hidden-lap-and-up account">
	<div class="header-me container">
		{%- if customer -%}

		<div class="account-box">
			<h3 class="mobile-account__title">My Service</h3>
			<div class="card__linklist ">
				<left-nav-list>
					<div class="render_nav_list d-flex mobile-account__nav">
						{%- for block in section.blocks -%}
						{%- if block.settings.nav_show -%}
						<div class="card__linklist-item link" style="cursor:pointer ;"
							data-right="{{block.settings.nav_right}}">
							<span> {%- render 'icon', icon: {{block.settings.nav_icon}} -%}</span>
							<span> {{block.settings.nav_title}} </span>
						</div>
						{%- endif -%}
						{%- endfor -%}
						<a href="{{ routes.account_logout_url }}" class="card__linklist-item link" data-no-instant>{% render 'icon',
							icon :"account-logout" %} {{ 'customer.general.logout' | t }}</a>
					</div>
				</left-nav-list>
			</div>
		</div>

		{%- else -%}
		<ul class="no-logged">
			<li class="text-l"><a href="{{ routes.account_login_url }}?return_url={{ page.url }}">LOGIN &gt;</a></li>
			<li class="text-r"><a href="{{ routes.account_register_url }}">CREATE ACCOUNT&gt;</a></li>
		</ul>
		{%- endif -%}
	</div>

</section>

