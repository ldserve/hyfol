<script>
	async function post_data(url, data) {
		const response = await fetch(`https://api-dev.leandow-technology.com/api/${url}`, {
			method: 'POST',
			mode: 'cors',
			cache: 'no-cache',
			credentials: 'same-origin',
			headers: {
				'Content-Type': 'application/json',
				"siteValue": "leandowTest",
				"Access-Control-Allow-Origin": "*"
			},
			redirect: 'follow',
			referrerPolicy: 'no-referrer',
			body: JSON.stringify(data)
		});
		return response.json();
	}

	function getCollectList(event) {

		var pageSectionId = document.querySelector(".page__sub-header").parentElement.getAttribute("data-section-id")
		const collectList = {
			current: 1,
			customerId: "{{customer.id}}",
			size: 100
		}
		post_data("customerCollectionProduct/collectionProductList", collectList)
			.then(res => {
				if (res.data.records.length < 1) {
					document.querySelector('.collect-body').style.display = "block"
					document.querySelector(".wish_loading").style.display = "none"
					return
				}
				// document.querySelector(".collection-num").innerHTML = res.data.total
				var ids = ""
				res.data.records.forEach((item, index) => {
					if (index === 0) {
						ids = `id:${item.productId}`
					} else {
						ids = ids + " OR " + `id:${item.productId}`
					}
				});
				fetch(`/search?q=${ids}&type=product`)
					.then((rs) => {
						return rs.text()
					})
					.then((rs) => {
						document.querySelector(".wish_loading").style.display = "none"

						let dom = new DOMParser().parseFromString(rs, 'text/html');

						dom.querySelector(".collection__toolbar").style.display = "none"

						document.querySelector('.collect').appendChild(dom.querySelector(
							".collection__dynamic-part"))

						document.querySelector('.collect').appendChild(dom.querySelector(
							".modal__dialog").parentElement)

						if (document.querySelector('.pagination__nav')) {
							document.querySelector('.pagination__nav').querySelectorAll(".pagination__nav-item")
								.forEach((item, index) => {
									item.addEventListener("click", nextCollectList)
								})
						}


						const removeNodes = document.querySelectorAll(".collect .add-collect")
						document.querySelector(".collect-body").style.display = "none"
						removeNodes.forEach((item, index) => {
							item.addEventListener("click", () => {
								document.querySelectorAll(
									`.collect .product-list .product-item`)[index].style
									.display = "none";
							})
						})
					})
			});
	}
	if ("{{ customer.id}}") {
		getCollectList()
	}


	function nextCollectList(event) {
		if (event) {
			event.preventDefault()
		}
		document.querySelector(".wish_loading").style.display = "flex"
		document.querySelector(".product-list--collection").style.display = "none"

		document.querySelectorAll(".pagination__nav-item").forEach((item) => {
			item.className = "pagination__nav-item link"
		})
		event.target.className = "pagination__nav-item link is-active"


		const collectList = {
			current: event.target.innerHTML * 1,
			customerId: "{{customer.id}}",
			size: 24
		}
		post_data("customerCollectionProduct/collectionProductList", collectList)
			.then(res => {


				if (res.data.records.length < 1) {
					document.querySelector('.collect-body').style.display = "block"
					return
				}
				var ids = ""

				res.data.records.forEach((item, index) => {
					if (index === 0) {
						ids = `id:${item.productId}`
					} else {
						ids = ids + " OR " + `id:${item.productId}`
					}
				});
				fetch(`/search?q=${ids}&type=product`)
					.then((rs) => {
						return rs.text()
					})
					.then((rs) => {
						let dom = new DOMParser().parseFromString(rs, 'text/html');

						document.querySelector(".product-list").remove()
						//  document.querySelector('.collection__dynamic-part').appendChild()
						document.querySelector('.collection__dynamic-part').insertBefore(dom.querySelector(
							".product-list"), document.querySelector(".collection__toolbar")
							.nextSibling);

						const removeNodes = document.querySelectorAll(".collect .add-collect")
						document.querySelector(".collect-body").style.display = "none"
						removeNodes.forEach((item, index) => {
							item.addEventListener("click", () => {
								document.querySelectorAll(
									`.collect .product-list .product-item`)[index].style
									.display = "none";
							})
						})
						document.querySelector(".wish_loading").style.display = "none"
						document.querySelector(".product-list--collection").style.display = "flex"
					})
			});
	}


</script>

