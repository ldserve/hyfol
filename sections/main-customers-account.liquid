<script>
	~ function () {
		let getDataFlag = false;
		let ajaxObj = ''
		let currentPage = ''
		function getData(url, fn) {
			// if (getDataFlag) return;
			getDataFlag = true;
			let http = new XMLHttpRequest();
			http.open('GET', url);
			http.send();
			http.onreadystatechange = () => {
				if (http.readyState == 4) {
					getDataFlag = false;
					if (http.status == 200 && !getDataFlag) {
						fn(http.responseText);
					}
				}
			}
			ajaxObj = http
		}

		function loading(bool) {
			let loading = document.querySelector('#loading')
			let body_me = document.querySelector('#body_me')
			let nullbox = document.querySelector('#order_null')
			if (bool) {
				loading.style.display = 'block';
				body_me.innerHTML = ''
				nullbox.style.display = 'none'
			} else {
				loading.style.display = 'none';
			}
		}

		function upData(data) {
			loading(false);
			let body_me = document.querySelector('#body_me')
			body_me.style.display = 'block'
			body_me.innerHTML = data
			isnull(data)
		}

		function isnull(data) {

			let fag = new DOMParser().parseFromString(data, 'text/html').querySelectorAll('.card .order-box')
			let nullbox = document.querySelector('#order_null')
			let body_me = document.querySelector('#body_me')
			if (!fag.length) {
				nullbox.style.display = 'block'
				body_me.style.display = 'none'
			} else {
				nullbox.style.display = 'none'
			}
		}

		function event() {
			let orders_list = document.querySelector("#orders-list")
			let orders_li = document.querySelectorAll("#orders-list>li")
			let post = document.querySelector('#post');
			let body_me = document.querySelector('#body_me')
			let nullbox = document.querySelector('#order_null')
			orders_list.onclick = (ev) => {
				let target = ev.target.tagName === "LI" ? ev.target : ev.target.parentElement;
				let item = target.dataset.item;
				if (currentPage === item) return
				currentPage = item
				if (target.tagName === "LI") {
					for (let i = 0; i < orders_li.length; i++) {
						orders_li[i].classList.remove('current')
					}
					target.classList.add('current')

					try { ajaxObj.abort() } catch (error) { }
					if (item !== 'post-sale') {
						post.style.display = 'none'
						loading(true);
						let url = '?section_id=my-' + item;
						getData(url, upData)
					} else {
						loading(false);
						post.style.display = 'block'
						body_me.style.display = 'none'
						nullbox.style.display = 'none'
					}
				}

			}
		}

		window.onhashchange = function (e) {
			try {
				var url = window.location.hash.split("+")[1].toLowerCase()
				document.querySelectorAll('#orders-list [data-item]').forEach(item => item.classList.remove('current'))
				const nav = document.querySelector(`#orders-list [data-item="orders-${url}"]`)
				nav.classList.add('current')
				getData('?section_id=my-'+nav.dataset.item, upData)
			} catch (error) {
			}

		}

		function init() {
			let url = '?section_id=my-orders-all';
			let nullbox = document.querySelector('#order_null')
			nullbox.style.display = 'none'
			getData(url, upData)
		}
		event();
		init();

		function bindEvent(container,target,call){
			let content=document.querySelector(container)
			content.addEventListener('click',(e)=>{
				if(!e.target.classList.contains(target)) return
				e.preventDefault()
				call&&call(e,content,target)
				
			})

		}
		
		function deleteAddresses() {
			function getFormData(form) {
				var hash = [];
				for (var i = 0, len = form.elements.length; i < len; i++) {
					var formElement = form.elements[i];
					if (formElement.name === '' || formElement.disabled) {
						continue;
					}
					if (formElement.name && !formElement.disabled && (formElement.checked || /select|textarea/i.test(formElement.nodeName) || /hidden|text|search|tel|url|email|password|datetime|date|month|week|time|datetime-local|number|range|color/i.test(formElement.type))) {
						hash.push(formElement.name + '=' + formElement.value)
					}
				}

				return hash.join('&');
			}

			document.querySelector('.form__submit[data-submitForm]').onclick = function (e) {
				e.preventDefault()
				const form = e.target.closest('form')
				const formData = getFormData(form)
				fetch('/account/addresses', {
					method: "POST",
					headers: { 'Content-Type': 'application/x-www-form-urlencoded' },
					body: formData
				}).then(response => {
					location.reload()

				})

			}

			document.querySelector('.address-list').addEventListener('click', ({ target }) => {
				if (!target.dataset.target) return
				if (confirm("Are you sure you wish to delete this address?")) {
					fetch(target.dataset.target, { method: "POST", headers: { 'Content-Type': 'application/x-www-form-urlencoded' }, body: '_method=delete' }).then(response => {
						location.reload()
					})
				}
			})
		}
		deleteAddresses()

