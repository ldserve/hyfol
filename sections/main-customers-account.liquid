			{% if order.financial_status == 'fulfilled' %}
			allPrice += ('{{ order.total_price }}' * 1) || 0
			{% endif %}
			{% endfor %}
			let point = Math.round(allPrice / 100)
			document.querySelector(".account-points").innerHTML = point
		}
